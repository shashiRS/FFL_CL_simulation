    ###########################
    # Description texcoord:  ##
    # 01 upper left corner   ##
    # 11 upper right corner  ##
    # 10 lower right corner  ##
    # 00 lower left corner   ##
    # Coord.Sys: x y z       ##
    ###########################

    gl texenv mode replace
    gl begin quads
    gl   texcoord 0 1  vertex  20. 97. 0
    gl   texcoord 1 1  vertex  23 97 0
    gl   texcoord 1 0  vertex  23 94 0
    gl   texcoord 0 0  vertex  20 94 0
    gl end

    gl popmatrix
    gl matrixmode projection
    gl popmatrix
    gl matrixmode modelview
	
	#####VWall######
	
