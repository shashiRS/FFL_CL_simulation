    # Description texcoord:  ##
    # 01 upper left corner   ##
    # 11 upper right corner  ##
    # 10 lower right corner  ##
    # 00 lower left corner   ##
    # Coord.Sys: x y z       ##
    ###########################

    gl texenv mode replace
    gl begin quads
    gl   texcoord 0 1  vertex  29. 95. 0
    gl   texcoord 1 1  vertex  70 95 0
    gl   texcoord 1 0  vertex  70 81 0
    gl   texcoord 0 0  vertex  29 81 0
    gl end

    gl popmatrix
    gl matrixmode projection
    gl popmatrix
    gl matrixmode modelview
	
	#####StrTrqreq-orange######
	
    gl matrixmode projection
    gl pushmatrix
    gl loadidentity
    #Screen is divided into 100*100 "points"#
    gl ortho 0 100. 0 100. -1. 1.
    gl matrixmode modelview
    gl pushmatrix
    gl loadidentity

    gl enable texture_2d
  
	gl bindtexture 2d $orange

    ###########################
    # Description texcoord:  ##
    # 01 upper left corner   ##
    # 11 upper right corner  ##
    # 10 lower right corner  ##
    # 00 lower left corner   ##
    # Coord.Sys: x y z       ##
    ###########################

	if {$Qu(LSCA.staticSteering.steeringTorqueRequestLsca)>=0 && $Qu(LSCA.staticSteering.steeringTorqueRequestLsca)<= 8 } {
	
	set lx [expr {($Qu(LSCA.staticSteering.steeringTorqueRequestLsca)*1.125)+ 52}]
	set rx [expr {($Qu(LSCA.staticSteering.steeringTorqueRequestLsca)*1.125)+ 45}]
	
	} elseif {$Qu(LSCA.staticSteering.steeringTorqueRequestLsca)<0 && $Qu(LSCA.staticSteering.steeringTorqueRequestLsca)>= -8} { 
	
	set slx [expr {($Qu(LSCA.staticSteering.steeringTorqueRequestLsca)*(-1.125))- 52}]
	set srx [expr {($Qu(LSCA.staticSteering.steeringTorqueRequestLsca)*(-1.125))- 45}]
	
	set lx [expr abs($slx)]
	set rx [expr abs($srx)]
	
	
	} else {
	set lx 52.00
	set rx 45.00
	}
	
	
    gl texenv mode replace
    gl begin quads
    gl   texcoord 0 1  vertex  $lx 93. 0
    gl   texcoord 1 1  vertex  $rx 93 0
    gl   texcoord 1 0  vertex  $rx 88.5 0
    gl   texcoord 0 0  vertex  $lx 88.5 0
    gl end

    gl popmatrix
    gl matrixmode projection
    gl popmatrix
    gl matrixmode modelview
}

proc DrawVehicle {} {
	global Qu Geo
	
	DrawObj
	DrawROIVWall
	DrawROIStrP
	DrawROIBrake

	CurrSteerTrajleft
	CurrSteerTrajright
	
 if {$Qu(LSCA.staticSteering.steerProp.isActive_nu) > 0 } { 	
	PropSteerTrajleft
	PropSteerTrajright 
	}
		
 if {$Qu(LSCA.staticSteering.virtWall.isActive_nu) > 0 } { 	
	PropSteerVWallleft
	PropSteerVWallright 
	}	
	
	if {$Qu(LSCA.staticSteering.steerProp.Traces.TracesActualSize)>0} {
		LTracesPlotleft
		LTracesPlotright
		
		RTracesPlotleft
		RTracesPlotright
	}
}

proc DrawObj {} {

global Qu Geo

for {set r 0} {$r < $Qu(LSCA.staticSteering.steerProp.Objects.objectsActualSize)} {incr r} {
	
	gl pushmatrix
	gl disable lighting		
	gl color 1.0 1.0 0.0 1.0
	gl linewidth 3
	gl begin line_loop
	
	for {set a 0} {$a< $Qu(LSCA.staticSteering.steerProp.Objects.objectsAfterRoi_$r.coordsActualSize)} {incr a} {	
		gl vertex [expr {1.067 + $Qu(LSCA.staticSteering.steerProp.Objects.objectsAfterRoi_$r.coord_$a.x)}] $Qu(LSCA.staticSteering.steerProp.Objects.objectsAfterRoi_$r.coord_$a.y) 1.8
	}
	
	gl end
	gl enable lighting
	gl popmatrix
	}
	
	}

proc DrawROIBrake {} {

	global Qu Geo
    gl pushmatrix
    gl disable lighting
	
    gl color 1.0 0.0 0.0 1.0
    gl linewidth 2
	gl linestipple 1 0x00FF
	gl enable line_stipple
	gl begin line_strip
    
    gl vertex [expr $Qu(LSCA.staticBraking.roi.rightTop_x)+(1.067)] $Qu(LSCA.staticBraking.roi.rightTop_y) 0.2
	gl vertex [expr $Qu(LSCA.staticBraking.roi.rightBottom_x) - (-1.067)] $Qu(LSCA.staticBraking.roi.rightBottom_y) 0.2
	gl vertex [expr $Qu(LSCA.staticBraking.roi.leftBottom_x) - (-1.067)] $Qu(LSCA.staticBraking.roi.leftBottom_y) 0.2
	gl vertex [expr $Qu(LSCA.staticBraking.roi.leftTop_x)+ (1.067)] $Qu(LSCA.staticBraking.roi.leftTop_y) 0.2
	gl vertex [expr $Qu(LSCA.staticBraking.roi.rightTop_x)+(1.067)] $Qu(LSCA.staticBraking.roi.rightTop_y) 0.2
	
    gl end
    gl enable lighting
    gl popmatrix
    gl disable polygon_offset_fill
    gl disable polygon_offset_line
	gl disable line_stipple

}

