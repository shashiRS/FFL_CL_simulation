proc DrawOverlay {} {
 
     global Qu
    ####################
    # Text ##
    ####################
 
    gl matrixmode projection
    gl pushmatrix
    gl loadidentity
    #Screen is divided into 50x50 "points"#
    gl ortho 0 50. 0 50. -1. 1. 
    gl matrixmode modelview
    gl pushmatrix
    gl loadidentity
    gl enable texture_2d
	

    #EBA
    gl color 1 1 0 1
	OGL drawtex sans-bold "EBA" -left -pos 40 47 0 -dir 2 0 0 -up 0 2 0 
	
	#steerProp
    gl color 1 1 0 1
	OGL drawtex sans-bold "Steer Prop" -left -pos 0 47 0 -dir 2 0 0 -up 0 2 0 
   
	 #virtual Wall
     gl color 1 1 0 1
     OGL drawtex sans-bold "VirtWall" -left -pos 20 47 0 -dir 2 0 0 -up 0 2 0

	 #AUP
           gl color 1 1 0 1
          OGL drawtex sans-bold "AUP" -left -pos 40 45 0 -dir 2 0 0 -up 0 2 0
	
     		 
	# SteeringTorque
		gl color 0.9 0.6 0.2 1
		set strtrq [format %.2f $Qu(LSCA.staticSteering.steeringTorqueRequestLsca)]
		OGL drawtex sans "SteerTrq = $strtrq Nm" -left -pos 17 43 0 -dir 2 0 0 -up 0 2 0 
	
		
   gl disable texture_2d
   gl popmatrix
   gl matrixmode projection
   gl popmatrix
   gl matrixmode modelview

    ####################
    # Pictures ##
    ####################

    variable tex_1
    variable tex_2
    variable tex_3
	
	variable green
	variable orange
    
    gl matrixmode projection
    gl pushmatrix
    gl loadidentity
    ##Screen is divided into 100*100 "points"##
    gl ortho 0 100. 0 100. -1. 1.
    gl matrixmode modelview
    gl pushmatrix
    gl loadidentity

    gl enable texture_2d

    #default picture / Status 1
    if {$Qu(LSCA.staticBraking.EbaOn) == 0 } {
	gl bindtexture 2d $tex_1 }

    if {$Qu(LSCA.staticBraking.EbaOn) > 0 && $Qu(LSCA.staticBraking.EbaActive) <= 0 } { 
    gl bindtexture 2d $tex_2 }

