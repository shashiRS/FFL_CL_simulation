    # Description texcoord:  ##
    # 01 upper left corner   ##
    # 11 upper right corner  ##
    # 10 lower right corner  ##
    # 00 lower left corner   ##
    # Coord.Sys: x y z       ##
    ###########################

    gl texenv mode replace
    gl begin quads
    gl   texcoord 0 1  vertex  88. 93. 0
    gl   texcoord 1 1  vertex  91 93 0
    gl   texcoord 1 0  vertex  91 90 0
    gl   texcoord 0 0  vertex  88 90 0
    gl end

    gl popmatrix
    gl matrixmode projection
    gl popmatrix
    gl matrixmode modelview
	
	#####StrTrqreq-base######
	
    gl matrixmode projection
    gl pushmatrix
    gl loadidentity
    #Screen is divided into 100*100 "points"#
    gl ortho 0 100. 0 100. -1. 1.
    gl matrixmode modelview
    gl pushmatrix
    gl loadidentity

    gl enable texture_2d
  
	gl bindtexture 2d $green

