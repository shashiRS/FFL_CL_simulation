proc CurrSteerTrajleft {} {

	global Qu Geo
	
	if {$Qu(LSCA.staticSteering.currentSteering) >= 0} {
	
	set ref_x1 1.096
	set ref_y1 0.890
    
	} else {
	
	set ref_x1 4.2
	set ref_y1 0.85
	
	}
		
	if {$Qu(LSCA.staticSteering.currentSteering) == 0} {
	
	gl pushmatrix
    gl disable lighting
	gl color 1.0 1.0 0.0 1.0	
	gl linewidth 3
	gl begin line_strip	
	
	gl vertex $ref_x1 $ref_y1 1
	gl vertex [expr {$Qu(LSCA.staticSteering.steerProp.roiPlot.rightBottom_x) + 1.067}] $ref_y1 1
	
	gl end
	gl enable lighting
	gl popmatrix
		
   } else {	
	
	set Propsteer [expr $Qu(LSCA.staticSteering.currentSteering) * 0.0174]

	set ICRx 0 	
	set ICRy [expr 2.787/[expr {atan ($Propsteer)}]]
		
	set r [expr { sqrt ([expr {pow ($ref_x1,2)}] + [expr {pow ($ICRy - $ref_y1,2)}])}]
	
	for {set a 0} {$a <= 360} {incr a} {
	
	set theta [expr {(2*3.1415926*$a)/360}]
	
	set x [expr {$r * cos($theta)}]
	set y [expr {$r * sin($theta)}]
		
	if {$x >= [expr ($Qu(LSCA.staticSteering.steerProp.roiPlot.leftBottom_x))+ (1.067)] && $x <= [expr {$Qu(LSCA.staticSteering.steerProp.roiPlot.rightBottom_x) + 1.067}] && [expr {$ICRy-$y}] <= $Qu(LSCA.staticSteering.steerProp.roiPlot.leftTop_y) && [expr {$ICRy-$y}] >=  $Qu(LSCA.staticSteering.steerProp.roiPlot.leftBottom_y)} {
	
	lappend Curve_CL [concat $x [expr ($ICRy-$y)]]

	set c1 [lsort -real -index 0 $Curve_CL]
	set m [lsort -real -index 1 $c1] 

	}
	}
	
	foreach elm $m {
			gl pushmatrix
			gl disable lighting	
			gl begin line_strip
			gl linewidth 2
			gl color 1.0 1.0 0.0 1.0
			gl vertex [lindex $elm 0] [lindex $elm 1] 0.8 
    }
	
		gl end
		gl enable lighting
		gl popmatrix
}
}

