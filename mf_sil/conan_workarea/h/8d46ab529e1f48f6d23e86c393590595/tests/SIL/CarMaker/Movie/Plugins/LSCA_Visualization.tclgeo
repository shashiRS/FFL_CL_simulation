    # 11 upper right corner  ##
    # 10 lower right corner  ##
    # 00 lower left corner   ##
    # Coord.Sys: x y z       ##
    ###########################

    gl texenv mode replace
    gl begin quads
    gl   texcoord 0 1  vertex  88. 97. 0
    gl   texcoord 1 1  vertex  91 97 0
    gl   texcoord 1 0  vertex  91 94 0
    gl   texcoord 0 0  vertex  88 94 0
    gl end

    gl popmatrix
    gl matrixmode projection
    gl popmatrix
    gl matrixmode modelview
	
	#####Steer Prop######
	
