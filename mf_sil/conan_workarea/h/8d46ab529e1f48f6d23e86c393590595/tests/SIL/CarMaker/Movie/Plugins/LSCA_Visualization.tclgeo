    gl loadidentity

    gl enable texture_2d

   #default picture / Status 1#
    if {$Qu(LSCA.staticSteering.virtWall.isOn_nu) == 0 } {
	gl bindtexture 2d $tex_1  }

    if {$Qu(LSCA.staticSteering.virtWall.isOn_nu) > 0 && $Qu(LSCA.staticSteering.virtWall.isActive_nu) <= 0 } { 
    gl bindtexture 2d $tex_2 }

    if {$Qu(LSCA.staticSteering.virtWall.isActive_nu) > 0 } { 
    gl bindtexture 2d $tex_3 }

    ###########################
    # Description texcoord:  ##
    # 01 upper left corner   ##
    # 11 upper right corner  ##
    # 10 lower right corner  ##
    # 00 lower left corner   ##
    # Coord.Sys: x y z       ##
    ###########################

    gl texenv mode replace
    gl begin quads
    gl   texcoord 0 1  vertex  55. 97. 0
    gl   texcoord 1 1  vertex  58 97 0
    gl   texcoord 1 0  vertex  58 94 0
    gl   texcoord 0 0  vertex  55 94 0
    gl end

    gl popmatrix
    gl matrixmode projection
    gl popmatrix
    gl matrixmode modelview
	
	#####AUP######
	
