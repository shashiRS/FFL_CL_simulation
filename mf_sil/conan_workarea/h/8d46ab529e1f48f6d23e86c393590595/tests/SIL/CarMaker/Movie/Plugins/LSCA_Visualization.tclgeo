    gl pushmatrix
    gl loadidentity
    ##Screen is divided into 100*100 "points"##
    gl ortho 0 100. 0 100. -1. 1.
    gl matrixmode modelview
    gl pushmatrix
    gl loadidentity

    gl enable texture_2d

   # default picture / Status 1
     if {$Qu(LSCA.staticSteering.steerProp.isOn_nu) == 0 } {
	gl bindtexture 2d $tex_1 }

    if {$Qu(LSCA.staticSteering.steerProp.isOn_nu) > 0 && $Qu(LSCA.staticSteering.steerProp.isActive_nu) <= 0 } { 
    gl bindtexture 2d $tex_2 }

